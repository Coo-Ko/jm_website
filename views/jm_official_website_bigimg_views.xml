<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="form_jm_official_website_bigimg">
      <field name="name">官网首页大图form试图</field>
      <field name="model">jm.official.website.bigimg</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name="image" widget="image" class="oe_avatar"/>
              </group>
              <group>
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>


    <record id="tree_jm_official_website_bigimg" model="ir.ui.view">
        <field name="name">官网首页大图tree试图</field>
        <field name="model">jm.official.website.bigimg</field>
        <field name="arch" type="xml">
            <tree>
              <field name="image"/>
            </tree>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_jm_official_website_bigimg">
      <field name="name">官网首页大图</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.official.website.bigimg</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
      

  </data>
</openerp>