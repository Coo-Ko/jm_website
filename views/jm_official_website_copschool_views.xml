<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="form_jm_official_website_copschool">
      <field name="name">合作院校详情form试图</field>
      <field name="model">jm.official.website.copschool</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name = 'name' required="1"/>
                <field name = 'type' required="1"/>
                <field name = 'image1' widget="image" class="oe_avatar"/>
                <field name = 'image2' widget="image" class="oe_avatar"/>
              </group>
              <group>
              </group>
            </group>
            <group>
              <field name="desc"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>


    <record id="tree_jm_official_website_copschool" model="ir.ui.view">
        <field name="name">合作院校详情tree试图</field>
        <field name="model">jm.official.website.copschool</field>
        <field name="arch" type="xml">
            <tree>
              <field name="name"/>
              <field name="type"/>
            </tree>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_jm_official_website_copschool">
      <field name="name">合作院校详情</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.official.website.copschool</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.ui.view" id="form_jm_official_website_cop_type">
      <field name="name">合作院校类别form试图</field>
      <field name="model">jm.official.website.cop.type</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name = 'name' required="1"/>
              </group>
              <group>
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>


    <record id="tree_jm_official_website_cop_type" model="ir.ui.view">
        <field name="name">合作院校类别tree试图</field>
        <field name="model">jm.official.website.cop.type</field>
        <field name="arch" type="xml">
            <tree>
              <field name="name"/>
            </tree>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_jm_official_website_cop_type">
      <field name="name">合作院校类别</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.official.website.cop.type</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
      

  </data>
</openerp>