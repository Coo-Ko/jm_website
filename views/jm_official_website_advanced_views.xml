<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="form_jm_official_website_advanced_team">
      <field name="name">官网先进集体form试图</field>
      <field name="model">jm.official.website.advanced.team</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name = 'name' required="1"/>
                <field name = 'desc' required="1"/>
                <field name="image" widget="image" class="oe_avatar"/>
              </group>
              <group>
              </group>
            </group>

          </sheet>
        </form>
      </field>
    </record>


    <record id="tree_jm_official_website_advanced_team" model="ir.ui.view">
        <field name="name">官网先进集体tree试图</field>
        <field name="model">jm.official.website.advanced.team</field>
        <field name="arch" type="xml">
            <tree>
              <field name="name"/>
              <field name="desc"/>
            </tree>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_jm_official_website_advanced_team">
      <field name="name">官网先进集体</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.official.website.advanced.team</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>



    <record model="ir.ui.view" id="form_jm_official_website_advanced_person">
      <field name="name">官网先进个人form试图</field>
      <field name="model">jm.official.website.advanced.person</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name = 'name' required="1"/>
                <field name="image" widget="image" class="oe_avatar"/>

              </group>
              <group>
              </group>
            </group>
            <group>
              <field name = 'desc' widget="html"/>
            </group>

          </sheet>
        </form>
      </field>
    </record>


    <record id="tree_jm_official_website_advanced_person" model="ir.ui.view">
        <field name="name">官网先进个人tree试图</field>
        <field name="model">jm.official.website.advanced.person</field>
        <field name="arch" type="xml">
            <tree>
              <field name="name"/>
              <field name="desc"/>
            </tree>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_jm_official_website_advanced_person">
      <field name="name">官网先进个人</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.official.website.advanced.person</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
    
    
    


  </data>
</openerp>